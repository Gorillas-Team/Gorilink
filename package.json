{
  "name": "gorilink",
  "version": "1.1.4",
  "main": "./src/index.js",
  "types": "./typings/index.d.ts",
  "exports": {
    ".": [
      {
        "require": "./src/index.js",
        "import": "./esm/gorilink.mjs"
      },
      "./src/index.js"
    ],
    "./esm": "./esm/gorilink.mjs"
  },
  "license": "ISC",
  "scripts": {
    "test:ts": "tsc",
    "lint": "eslint src/ examples/",
    "lint:typings": "eslint typings/index.d.ts",
    "lint:fix": "eslint --fix src/ examples/",
    "jsdoc": "jsdoc -c .jsdocs.json --verbose -r src/ -R README.md"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gorillas-Team/Gorilink.git"
  },
  "homepage": "https://gorillas-team.github.io/Gorilink/",
  "bugs": {
    "url": "https://github.com/Gorillas-Team/Gorilink/issues"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@types/ws": "^7.2.6",
    "discord.js": "^12.2.0",
    "eslint": "^6.8.0",
    "jsdoc": "^3.6.4",
    "jsdoc-template": "https://github.com/braintree/jsdoc-template",
    "nodemon": "^2.0.3",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@discordjs/collection": "^0.1.5",
    "node-fetch": "^2.6.0",
    "ws": "^7.2.5"
  },
  "description": "Easy lavalink wrapper for Discord.js. (soon eris)",
  "directories": {
    "example": "examples"
  },
  "keywords": [
    "lavalink",
    "discord.js",
    "discord music bot",
    "discord music",
    "lavalink client",
    "discord bot"
  ],
  "contributors": [
    "Psyka (https://github.com/Psykka)",
    "zMigueel (https://github.com/zMigueel)"
  ]
}
